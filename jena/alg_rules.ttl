###############################
# decision-tree action-boundaries-polyfill
###############################

[action-boundary-polyfill-begin_of:
	(?a rdf:type my:action),
	noValue(?x my:begin_of ?a) ,

	makeNamedSkolem(?b, my:boundary, ?a, "begin_of"),
	 ->
	 (?b rdf:type my:boundary) ,
	 (?b my:begin_of ?a) ,
]

[action-boundary-polyfill-end_of:
	(?a rdf:type my:action),
	noValue(?x my:end_of ?a) ,

	makeNamedSkolem(?b, my:boundary, ?a, "end_of"),
	 ->
	 (?b rdf:type my:boundary) ,
	 (?b my:end_of ?a) ,
]

# this two ^ rules work good.



###############################
# stmt and expr
###############################


[connect_StmtEnd:

	(?st rdf:type my:stmt),
	(?b my:begin_of ?st),
	(?e my:end_of ?st),
	 ->
	 (?b my:always_consequent ?e),
	 (?b my:StmtEnd ?e),
]
[connect_ExprEnd:

	(?st rdf:type my:expr),
	(?b my:begin_of ?st),
	(?e my:end_of ?st),
	 ->
	 (?b my:always_consequent ?e),
	 (?b my:ExprEnd ?e),
]

[connect_InterruptActionEnd:

	(?st rdf:type my:interrupt_action),
	(?b my:begin_of ?st),
	makeNamedSkolem(?E, my:end, ?st),
	 ->
	 (?E my:halt_of ?st),
	 (?b my:always_consequent ?E),
	 (?b my:StmtEnd ?E),
	 (?E my:interrupt_origin ?E),
]


###############################
# decision-tree alternative helper
###############################

[alternative-to-alternative_multi_with_else:

	(?a rdf:type my:alternative),
	(?a my:branches_item ?b),
	(?b rdf:type my:else-if),
	(?a my:branches_item ?b2),
	(?b2 rdf:type my:else),
	 ->
	 (?a rdf:type my:alternative_multi_with_else),
]

[alternative-to-alternative_multi_without_else:

	(?a rdf:type my:alternative),
	(?a my:branches_item ?b),
	(?b rdf:type my:else-if),
	noValue(?b, my:next),
	 ->
	 (?a rdf:type my:alternative_multi_without_else),
]

[alternative-to-alternative_single_with_else:

	(?a rdf:type my:alternative),
	(?a my:branches_item ?b),
	(?b rdf:type my:if),
	(?b my:next ?b2),
	(?b2 rdf:type my:else),
	 ->
	 (?a rdf:type my:alternative_single_with_else),
]

[alternative-to-alternative_simple:

	(?a rdf:type my:alternative),
	(?a my:branches_item ?b),
	(?b rdf:type my:if),
	noValue(?b, my:next),
	 ->
	 (?a rdf:type my:alternative_simple),
]



###############################
# decision-tree helper
###############################


[branches_item-o-cond-to-parent_of:

	(?a my:branches_item ?b),
	(?b my:cond ?c)
	 -> (?a my:parent_of ?c)
]

[loop-cond-to-parent_of:

	(?Loop rdf:type my:loop),
	(?Loop my:cond ?c)
	 -> (?Loop my:parent_of ?c)
]

[seq-of-loop-to-loop_body:
# Обновляем тип следования для старых вопросов, в которых был использован просто тип sequence
	(?Loop rdf:type my:loop),
	(?block rdf:type my:sequence),
	(?Loop my:body ?block)
	 -> (?block rdf:type my:loop_body)
]


