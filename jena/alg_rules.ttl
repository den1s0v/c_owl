[role-for-func_body:
	(?action rdf:type my:sequence),
	(?parent rdf:type my:func),
	(?parent my:body ?action),
	 -> (?action my:has_role my:func_body)
]
[role-for-func_call:
	(?action rdf:type my:func_call),
#	(?parent rdf:type my:stmt_with_calls),
#	(?parent my:has_func_call ?action),
	 -> (?action my:has_role my:func_call)
]
[role-for-loop_body:
	(?action rdf:type my:sequence),
	(?parent rdf:type my:loop),
	(?parent my:body ?action),
	 -> (?action my:has_role my:loop_body)
]
[role-for-sequence_element:
	(?parent rdf:type my:sequence),
	(?parent my:body_item ?action),
	 -> (?action my:has_role my:sequence_element)
]
[role-for-alt_branch:
	(?action rdf:type my:alt_branch),
	 -> (?action my:has_role my:alt_branch)
]
[role-for-alt_cond:
	(?action rdf:type my:expr),
	(?parent rdf:type my:alt_branch),
	(?parent my:cond ?action),
	 -> (?action my:has_role my:alt_cond)
]
[role-for-loop_init:
	(?action rdf:type my:stmt),
	(?parent rdf:type my:loop),
	(?parent my:init ?action),
	 -> (?action my:has_role my:loop_init)
]
[role-for-loop_cond:
	(?action rdf:type my:expr),
	(?parent rdf:type my:loop),
	(?parent my:cond ?action),
	 -> (?action my:has_role my:loop_cond)
]
[role-for-loop_update:
	(?action rdf:type my:stmt),
	(?parent rdf:type my:loop),
	(?parent my:update ?action),
	 -> (?action my:has_role my:loop_update)
]


